  "main": "index.js",
  "scripts": {
    "fbuilder": "node ./node_modules/fock-builder/bin/index.js --start",
    "build": "tsc && npm run fbuilder"
  },
  "keywords": [
    "auto",
    "updater",
    "update",
    "helper"
  ],
  "bin": {
    "fockupdater": "./bin/index.js"
  },
  "author": "FOCKUSTY",
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",

    "config/configurator.config.js",
    "config/types.config.js",
    "config/validator.config.js",
    "config/configurator.config.d.ts",
    "config/types.config.d.ts",
    "config/validator.config.d.ts",

    "bin/index.js",
    "bin/types.js",
    
    "bin/commands/config.js",
    "bin/commands/libs.js",

    "managers/downloader.manager.js",
    "managers/package.manager.js",
    "managers/unarchiver.manager.js",
