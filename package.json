    "managers/package.manager.d.ts",
    "managers/unarchiver.manager.d.ts"
  ],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/tar-stream": "^3.1.3",
    "@types/yargs": "^17.0.33",
    "fock-builder": "^1.0.2-beta",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "node": "^23.5.0",
    "npm-api": "^1.0.1",
    "tar-stream": "^3.1.7",
    "yargs": "^17.7.2"
  }
